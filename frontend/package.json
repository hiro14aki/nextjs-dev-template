{
  "name": "nextjs-dev-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier '**/*.{js,json,ts,tsx}' --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "ress": "2.0.4",
    "styled-components": "5.1.1",
    "typescript": "3.9.6"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
    "@types/react": "16.9.41",
    "@types/styled-components": "5.1.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.20.3",
    "husky": "4.2.5",
    "lint-staged": "10.1.4",
    "prettier": "2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
